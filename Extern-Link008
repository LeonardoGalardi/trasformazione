<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0" xmlns:xalan="http://xml.apache.org/xslt">
	<!--*****************************************************************************
		Creazione form con campi di input hidden e loro valorizzazione da XML Host
		ma solo se Ã¨ presente un evento di tipo 'ESTERNO'
		Link a Lombarda Vita (LIFENET)
	      *****************************************************************************-->

	<xsl:template name = "Extern-Link0008" >

			<form method="post" name="formEventoEsterno" target="_blank" style="margin=0px;">
				<xsl:attribute name="action">
		  		<xsl:value-of select="/SERVICE/ENV/INTESAVITA"/>
		  		</xsl:attribute>  <!-- link esterno -->

				<!--
				Non viene passato nessun parametro,
				tutti i dati vengono presi dallo header http (impostati da WEBSEAL)
		      	-->

			</form>
	</xsl:template>

	<xsl:template name = "Button-Link0008">
		<input type="button" value="PASSA A INTESA VITA" class="formButton">
			<xsl:attribute name="onclick"><xsl:value-of select="'javascript:document.formEventoEsterno.submit();'"/></xsl:attribute>
		</input>
	</xsl:template>
</xsl:stylesheet>
